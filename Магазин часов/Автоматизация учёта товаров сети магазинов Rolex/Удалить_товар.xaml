<Window x:Class="Автоматизация_учёта_товаров_сети_магазинов_Rolex.Удалить_товар"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Автоматизация_учёта_товаров_сети_магазинов_Rolex"
        mc:Ignorable="d"
        Title="Удалить_товар" Height="186" Width="167">
    <Grid>
        <ComboBox x:Name="Выберите_товар2" HorizontalAlignment="Left" Margin="10,49,0,0" VerticalAlignment="Top" Width="142" SelectionChanged="Выберите_товар2_SelectionChanged"/>
        <Label x:Name="Выберите_товар1" Content="Выберите поставку, &#xA;которую хотите удалить" HorizontalAlignment="Left" Margin="10,2,0,0" VerticalAlignment="Top" Width="142"/>
        <Button x:Name="Удалить" Content="Удалить" HorizontalAlignment="Left" Margin="10,77,0,0" VerticalAlignment="Top" Width="142" Height="30" Click="Удалить_Click"/>
        <Button x:Name="Отмена" Content="Отмена" HorizontalAlignment="Left" Margin="10,112,0,0" VerticalAlignment="Top" Width="142" Height="30" Click="Отмена_Click"/>

    </Grid>
</Window>
